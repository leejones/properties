<h1>Listing property_types</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
  </tr>

<% @property_types.each do |property_type| %>
  <tr>
    <td><%=h property_type.name %></td>
    <td><%=h property_type.description %></td>
    <td><%= link_to 'Show', property_type %></td>
    <td><%= link_to 'Edit', edit_property_type_path(property_type) %></td>
    <td><%= link_to 'Destroy', property_type, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New property_type', new_property_type_path %>